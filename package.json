    "salesforce-alm": "^48.8.0"
  },
  "devDependencies": {
    "@salesforce/dev-config": "1.4.1",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/fs-extra": "^8.1.0",
    "@types/node": "^10.17.18",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "globby": "^8",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
